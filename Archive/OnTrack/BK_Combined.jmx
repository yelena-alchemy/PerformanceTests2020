              <BeanShellPreProcessor guiclass="TestBeanGUI" testclass="BeanShellPreProcessor" testname="BeanShell PreProcessor" enabled="true">
                <stringProp name="filename"></stringProp>
                <stringProp name="parameters"></stringProp>
                <boolProp name="resetInterpreter">false</boolProp>
                <stringProp name="script">int cardsPastDays = Integer.parseInt(vars.get(&quot;cardsPastDays&quot;));
int cardsCallNumber = Integer.parseInt(vars.get(&quot;cardsCallNumber&quot;));

/*
log.info(&quot;callCardsService: &quot;+vars.get(&quot;callCardsService&quot;));
log.info(&quot;cardsCallNumber: &quot;+cardsCallNumber);
log.info(&quot;cardPostId: &quot;+vars.get(&quot;cardPostId&quot;));
log.info(&quot;postFound: &quot;+postFound);
*/

if (cardsCallNumber &lt; cardsPastDays) {
	// the purpose of this is to mimic what the app does:
		// helps to try to get posts for today, if not found, it tries dates in the past up to 7 days, stops if posts are found

	// procure Start Date
	Calendar cal = Calendar.getInstance();
	cal.add(Calendar.DAY_OF_MONTH, -1*cardsCallNumber);
	cal.set(Calendar.HOUR_OF_DAY, 0);
	cal.set(Calendar.MINUTE, 0); 
	cal.set(Calendar.SECOND, 0);
	cal.set(Calendar.MILLISECOND, 0);

	// set var to be used by request sampler
	vars.put(&quot;cardsStartDate&quot;, &quot;&quot;+cal.getTime().getTime());

	// procure End Date
	if (0 == cardsCallNumber) {
		// don&apos;t ask for future stuff
		Date now = new Date();
		vars.put(&quot;cardsEndDate&quot;, &quot;&quot;+now.getTime());
	} else {
		cal.add(Calendar.DAY_OF_MONTH, 1);
		vars.put(&quot;cardsEndDate&quot;, &quot;&quot;+cal.getTime().getTime());
	}
} else {
	vars.put(&quot;callCardsService&quot;, &quot;false&quot;);
}
</stringProp>
