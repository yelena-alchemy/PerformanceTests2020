          <BeanShellSampler guiclass="BeanShellSamplerGui" testclass="BeanShellSampler" testname="RequestHelper" enabled="true">
            <stringProp name="BeanShellSampler.query">class RequestHelper{
void addBadge( ){	
	String cardId=vars.get(&quot;cardId&quot;);
	String name=vars.get(&quot;name&quot;);
	String topicCd=vars.get(&quot;topicCd&quot;);
	String sortOrder=vars.get(&quot;sortOrder&quot;);
	// Boolean value returned 
	Boolean isSystem=(new Boolean(vars.get(&quot;isSystem&quot;))).booleanValue();
	String payloadType = vars.get(&quot;payloadType&quot;);
	String mediaLinks = vars.get(&quot;mediaLinks&quot;);
	String payload = vars.get(&quot;payload&quot;);

		String badgePayload=&quot;{\&quot;badge\&quot;:&quot; 
		+ &quot;{\&quot;id\&quot;:&quot;+ cardId +&quot;,&quot;
		+ &quot;\&quot;holderType\&quot;:\&quot;user\&quot;,&quot;
		+ &quot;\&quot;name\&quot;:&quot; + &quot;\&quot;&quot; + name + &quot;\&quot;&quot; +&quot;,&quot;
		+ &quot;\&quot;elt\&quot;:30,&quot;
		+ &quot;\&quot;selfAssignable\&quot;:&quot; + false +  &quot;,&quot;
		+ &quot;\&quot;isSystem\&quot;:&quot; + isSystem + &quot;,&quot;
		+ &quot;\&quot;payload\&quot;:&quot;
		+ &quot;{\&quot;@class\&quot;:\&quot;&quot; + payloadType + &quot;,&quot;
		+ &quot;\&quot;topicCd\&quot;:&quot; + topicCd +&quot;},&quot;
		+ &quot;\&quot;sortOrder\&quot;:&quot; + sortOrder +&quot;,&quot;
		+ &quot;\&quot;category\&quot;:\&quot;certification\&quot;,&quot;
		+ &quot;\&quot;mediaLinks\&quot;:&quot; +  mediaLinks + &quot;,&quot;
		+ &quot;\&quot;&quot; + payloadType +&quot;\&quot;&quot;+ &quot;,&quot;
		+ &quot;\&quot;payload\&quot;:&quot; + payload + &quot;,&quot;
		+ &quot;}]&quot;;
		vars.put(&quot;BADGEPAYLOAD&quot;, badgePayload);
		log.error(badgePayload);
		
	}
	return this;
};

bsh.shared.RequestHelper= new RequestHelper();</stringProp>
